<?xml version="1.0" encoding="UTF-8"?>
<generalAdapterRun xmlns="http://www.wldelft.nl/fews" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.wldelft.nl/fews http://fews.wldelft.nl/schemas/version1.0/generalAdapterRun.xsd">
	<general>
		<rootDir>%REGION_HOME%/modules/respighi</rootDir>
		<workDir>%ROOT_DIR%/work</workDir>
		<exportDir>%ROOT_DIR%/input</exportDir>
		<exportDataSetDir>%ROOT_DIR%</exportDataSetDir>
		<exportIdMap>idRESPIGHI</exportIdMap>
		<importDir>%ROOT_DIR%/output</importDir>
		<importIdMap>idRESPIGHI</importIdMap>
		<dumpFileDir>%REGION_HOME%/DumpFiles</dumpFileDir>
		<dumpDir>%ROOT_DIR%/work</dumpDir>
		<diagnosticFile>%WORK_DIR%/pi_diagnostics.xml</diagnosticFile>
		<missVal>-999</missVal>
		<convertDatum>true</convertDatum>
	</general>
	<activities>
		<startUpActivities>
			<purgeActivity>
				<filter>%ROOT_DIR%/work</filter>
			</purgeActivity>
			<purgeActivity>
				<filter>%ROOT_DIR%/input/*</filter>
			</purgeActivity>
			<purgeActivity>
				<filter>%ROOT_DIR%/output/*</filter>
			</purgeActivity>
		</startUpActivities>
		<exportActivities>
			<exportStateActivity>
				<moduleInstanceId>$MODULEINSTANCE_INPUT$</moduleInstanceId>
				<stateExportDir>%ROOT_DIR%/input</stateExportDir>
				<stateConfigFile>%ROOT_DIR%/input/states.xml</stateConfigFile>
				<stateLocations type="file">
					<stateLocation>
						<readLocation>starting_heads_laag1.IDF</readLocation>
						<writeLocation>starting_heads_laag1.IDF</writeLocation>
					</stateLocation>
					<stateLocation>
						<readLocation>starting_heads_laag2.IDF</readLocation>
						<writeLocation>starting_heads_laag2.IDF</writeLocation>
					</stateLocation>
					<stateLocation>
						<readLocation>starting_heads_laag3.IDF</readLocation>
						<writeLocation>starting_heads_laag3.IDF</writeLocation>
					</stateLocation>
					<stateLocation>
						<readLocation>starting_heads_laag4.IDF</readLocation>
						<writeLocation>starting_heads_laag4.IDF</writeLocation>
					</stateLocation>
					<stateLocation>
						<readLocation>starting_heads_laag5.IDF</readLocation>
						<writeLocation>starting_heads_laag5.IDF</writeLocation>
					</stateLocation>
					<stateLocation>
						<readLocation>starting_heads_laag6.IDF</readLocation>
						<writeLocation>starting_heads_laag6.IDF</writeLocation>
					</stateLocation>
					<stateLocation>
						<readLocation>starting_heads_laag7.IDF</readLocation>
						<writeLocation>starting_heads_laag7.IDF</writeLocation>
					</stateLocation>
					<stateLocation>
						<readLocation>starting_heads_laag8.IDF</readLocation>
						<writeLocation>starting_heads_laag8.IDF</writeLocation>
					</stateLocation>
					<stateLocation>
						<readLocation>init_svat.inp</readLocation>
						<writeLocation>init_svat.inp</writeLocation>
					</stateLocation>
				</stateLocations>
				<stateSelection>
					<warmState>
						<stateSearchPeriod unit="hour" start="-72" end="0"/>
					</warmState>
				</stateSelection>
			</exportStateActivity>
			<exportDataSetActivity>
			<moduleInstanceId>RESPIGHI</moduleInstanceId></exportDataSetActivity>
		</exportActivities>
		<executeActivities>
			<executeActivity>
				<command>
					<executable>$HYDROMEDAH_PYTHON$</executable>
				</command>
				<arguments>
					<argument>%ROOT_DIR%/respighifews/adapter.py</argument>
					<argument>-configtoml</argument>
					<argument>%ROOT_DIR%/$ADAPTER_INI$</argument>
				</arguments>
				<timeOut>14400000</timeOut>
				<ignoreDiagnostics>false</ignoreDiagnostics>
				<ignoreExitCode>false</ignoreExitCode>
			</executeActivity>
		</executeActivities>
		<importActivities>
			<importNetcdfActivity>
				<importFile>output.nc</importFile>
				<timeSeriesSets>
					<timeSeriesSet>
						<moduleInstanceId>$MODULEINSTANCE_OUTPUT$</moduleInstanceId>
						<valueType>grid</valueType>
						<parameterId>gws.cm.mv.N</parameterId>
						<locationId>$RESPIGHI_GRID$</locationId>
						<timeSeriesType>simulated historical</timeSeriesType>
						<timeStep unit="nonequidistant" timeZone="GMT+1"/>
						<readWriteMode>add originals</readWriteMode>
					</timeSeriesSet>
				</timeSeriesSets>
			</importNetcdfActivity>
		</importActivities>
	</activities>
</generalAdapterRun>
